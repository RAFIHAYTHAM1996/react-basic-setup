{
  "name": "rafi-george",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 9968",
    "build": "set BUNDLE_ANALYZE=both&& next build",
    "clean": "rm -rf out .next",
    "export": "next export && cp -r ./{.htaccess,.htpasswd,.user.ini,forms} out",
    "deploy": "npm run rename && rm -rf ~/Desktop/Projects/RAFIHAYTHAM1996.github.io/* && cp -r out/* ~/Desktop/Projects/RAFIHAYTHAM1996.github.io/ && cd ~/Desktop/Projects/RAFIHAYTHAM1996.github.io && git add . && git commit -m 'Updates' && git push",
    "precommit": "lint-staged",
    "rename": "find ./out -depth -execdir rename 's/^_//' '{}' \\; | find ./out -type f -exec sed -Ei '' 's/_(app|error|next|ssgManifest|buildManifest)/\\1/g' {} \\;",
    "start": "set NODE_ENV=production&& node server",
    "static": "npm-run-all clean build export"
  },
  "dependencies": {
    "@fortawesome/fontawesome": "^1.1.8",
