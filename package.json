    "next": "9.3.1",
    "next-compose-plugins": "^2.2.0",
    "next-page-transitions": "^1.0.0-beta.2",
    "next-react-svg": "^1.1.0",
    "nextjs-fonts": "^0.22.0",
    "node-sass": "^4.13.1",
    "react": "16.13.1",
    "react-dom": "16.13.1",
